{
    "name": "hexlet/code",
    "description": "Demo project #2 for Hexlet",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "squizlabs/php_codesniffer": "^3.5",
        "docopt/docopt": "^1.0",
        "tightenco/collect": "^8.17"
    },
    "scripts": {
        "phpcs": "phpcs -- --standard=PSR12 bin src tests",
        "phpstan-src": "phpstan analyse -l 5 src tests"
    },
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/func_loader.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.58",
        "phpunit/phpunit": "^9.5"
    }
}
